{
  "compilerOptions": {
    "outDir": "./dist/",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "module": "es6",
    "target": "ES6",
    "jsx": "react",
    "allowJs": true,
    "suppressImplicitAnyIndexErrors": true,
  },
  "exclude": ["spec"]
}
